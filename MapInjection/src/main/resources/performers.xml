<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd ">

<bean id="guitar" class="com.zensar.spring.instruments.Guitar" ></bean>
<bean id="piano" class="com.zensar.spring.instruments.Piano" ></bean>
<bean id="drum" class="com.zensar.spring.instruments.Drum" ></bean>

<bean id="Neil" class="com.zensar.spring.performers.Instrumentalist">
<property name="instrumentSongMap">
<map>
<entry key-ref="piano" value="neele neele ambar pe "></entry>
<entry key-ref="guitar" value="kati patang "></entry>
<entry key-ref="drum" value="All of me "></entry>

</map>

</property>
</bean>


</beans>
